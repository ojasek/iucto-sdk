{
    "name": "ojasek/iucto-sdk",
    "type": "library",
    "description": "SDK library for iUcto",
    "keywords": [
        "ojasek",
        "iucto-sdk"
    ],
    "homepage": "https://github.com/ojasek/iucto-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Ondřej Jašek",
            "email": "jasek@fixitsoft.cz",
            "homepage": "https://github.com/ojasek",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.2",
        "guzzlehttp/guzzle": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Ojasek\\IuctoSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ojasek\\IuctoSdk\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
